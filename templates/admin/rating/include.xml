{* purpose of this template: ratings xml inclusion template in admin area *}
<rating id="{$item.id}" createdon="{$item.createdDate|dateformat}" updatedon="{$item.updatedDate|dateformat}">
    <id>{$item.id}</id>
    <ratingvalue>{$item.ratingvalue}</ratingvalue>
    <workflowState>{$item.workflowState|muticketObjectState:false|lower}</workflowState>
</rating>
