{* purpose of this template: labels xml inclusion template in user area *}
<label id="{$item.id}" createdon="{$item.createdDate|dateformat}" updatedon="{$item.updatedDate|dateformat}">
    <id>{$item.id}</id>
    <name><![CDATA[{$item.name}]]></name>
    <labelColor><![CDATA[{$item.labelColor}]]></labelColor>
    <workflowState>{$item.workflowState|muticketObjectState:false|lower}</workflowState>
</label>
